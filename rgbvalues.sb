Sensor.SetMode(3, 4)
Sensor.SetMode(2, 4)
While "True"
    valueRight = Sensor.ReadRaw(3, 3)
    valueLeft = Sensor.ReadRaw(2, 3)
    LCD.Clear()
    LCD.write(1, 1, valueLeft[0])
    LCD.write(20, 1, valueRight[0])
    LCD.write(1, 20, valueLeft[1])
    LCD.write(20, 20, valueRight[1])
    LCD.write(1, 40, valueLeft[2])
    LCD.write(20, 40, valueRight[2])
EndWhile