Sensor.SetMode(3, 4)
Sensor.SetMode(2, 4)
While "True"
    valueRight = Sensor.ReadRaw(3, 3)
    valueLeft = Sensor.ReadRaw(2, 3)
    LCD.Clear()
    LCD.write(1, 1, valueRight[0])
    LCD.write(1, 10, valueLeft[0])
    LCD.write(10, 1, valueRight[1])
    LCD.write(10, 10, valueLeft[1])
    LCD.write(20, 1, valueRight[2])
    LCD.write(20, 10, valueLeft[2])
EndWhile